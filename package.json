{
  "name": "acm-site",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "babel react/src -d react/build && npx webpack",
    "build-watch": "parallel -j 2 -- 'babel react/src -d react/build --watch' 'npx webpack --watch'",
    "start": "node ./bin/www",
    "build-start": "npm run build && npm start",
    "build-start-watch": "parallel -j 2 -- 'npm run build-watch' 'npm run start'",
    "bsw": "npm run build-start-watch"
  },
  "dependencies": {
    "babel-preset-es2015": "^6.24.1",
    "body-parser": "~1.18.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.15.5",
    "jquery": "^3.3.1",
    "mongoose": "^5.0.3",
    "morgan": "~1.9.0",
    "node-sass-middleware": "0.9.8",
    "pug": "2.0.0-beta11",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "serve-favicon": "~2.4.5"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "webpack": "^3.10.0"
  }
}
